{
  "name": "front-end-lespetisplats",
  "version": "1.0.0",
  "description": "npm package for Les petits plats Frontend project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack-serve": "webpack --watch --progress --mode development",
    "webpack-build": "webpack --mode production",
    "sass-build": "sass ./src/sass/main.scss:./dist/css/style.css --style compressed",
    "sass-watch": "sass --watch ./src/sass/main.scss:./dist/css/style.css",
    "watch": "npm-run-all --parallel webpack-serve sass-watch",
    "build": "npm-run-all --parallel webpack-build sass-build"
  },
